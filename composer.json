{
    "name": "donkeyworks/zendframework1-json",
    "description": "Zend Framework 1 JSON component",
    "type": "library",
    "keywords": [
        "framework",
        "zf1"
    ],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "donkeyworks/zendframework1-exception": "^1.0.0",
        "donkeyworks/zendframework1-xml": "^1.0.0",
        "donkeyworks/zendframework1-server": "^1.0.0"
    },
    "autoload": {
        "psr-0": {
            "Zend_Json_": "src/"
        },
        "classmap": [
            "src/Zend/Json.php"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "Zend_": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "phpstan/phpstan": "^0.9.2",
        "friendsofphp/php-cs-fixer": "^2.11",
        "maglnet/composer-require-checker": "^0.1.6 | ^0.2.1",
        "phpro/grumphp": "^0.14.0"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "scripts": {
        "test": "phpunit",
        "test-with-coverage": "phpunit --coverage-clover=clover.xml",
        "phpstan": "phpstan analyze -l4 -c phpstan.neon --no-progress ./ --ansi",
        "style-check": "php-cs-fixer fix --dry-run -vv"
    },
    "suggest": {
        "ext-json": "Can use built-in json_encode/json_decode functions if available",
        "ext-mbstring": "Will use mbstring extension for encoding/decoding if available"
    }
}
